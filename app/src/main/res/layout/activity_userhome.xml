<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackgroundMain"
    tools:context=".UserHome">

    <LinearLayout
        android:id="@+id/contFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <FrameLayout
            android:id="@+id/Fragments"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </FrameLayout>
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/sizeFooter"
        android:background="@color/colorFooterMain"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:id="@+id/footer"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/feedButton"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:clickable="true"
                android:focusable="true"
                android:onClick="onClick"
                android:orientation="vertical"
                tools:ignore="UsingOnClickInXml">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingHorizontal="@dimen/sizeContIconFooter"
                    tools:ignore="InefficientWeight">

                    <LinearLayout
                        android:id="@+id/footerHomeIcon"
                        android:layout_width="@dimen/sizeIconFooter"
                        android:layout_height="@dimen/sizeIconFooter"
                        android:background="@drawable/iconHome"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="TextContrastCheck" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/storeButton"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:clickable="true"
                android:focusable="true"
                android:onClick="onClick"
                android:orientation="vertical"
                tools:ignore="UsingOnClickInXml">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingHorizontal="@dimen/sizeContIconFooter"
                    tools:ignore="InefficientWeight">

                    <LinearLayout
                        android:id="@+id/footerStoreIcon"
                        android:layout_width="@dimen/sizeIconFooter"
                        android:layout_height="@dimen/sizeIconFooter"
                        android:background="@drawable/iconStore"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="TextContrastCheck" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/addButton"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingHorizontal="@dimen/sizeContIconFooter"
                    tools:ignore="InefficientWeight">

                    <LinearLayout
                        android:id="@+id/footerAddIcon"
                        android:layout_width="@dimen/sizeIconFooter"
                        android:layout_height="@dimen/sizeIconFooter"
                        android:background="@drawable/iconAdd"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="TextContrastCheck" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/menuButton"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingHorizontal="@dimen/sizeContIconFooter"
                    tools:ignore="InefficientWeight">

                    <LinearLayout
                        android:id="@+id/footerMenuIcon"
                        android:layout_width="@dimen/sizeIconFooter"
                        android:layout_height="@dimen/sizeIconFooter"
                        android:background="@drawable/iconMenu"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="TextContrastCheck" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/userHomeButton"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clickable="true"
                android:onClick="onClick"
                android:orientation="vertical"
                android:focusable="true"
                tools:ignore="UsingOnClickInXml">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    tools:ignore="InefficientWeight">

                    <LinearLayout
                        android:id="@+id/footerCircleUser"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/specialElemFooter"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.cardview.widget.CardView
                        android:id="@+id/footerCardView"
                        android:layout_width="@dimen/sizeContImageUserFooter"
                        android:layout_height="@dimen/sizeContImageUserFooter"
                        android:outlineAmbientShadowColor="@color/whiteTransparente0"
                        android:outlineSpotShadowColor="@color/whiteTransparente0"
                        app:cardCornerRadius="@dimen/sizeRadiusImageUser"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="MissingConstraints,UnusedAttribute">

                        <ImageView
                            android:id="@+id/footerImgPhotoUser"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@color/colorImgPerfilUser"
                            android:contentDescription="@string/descriptionImg"
                            android:rotation="@integer/sizeRotationImageUser"
                            android:scaleType="centerCrop"/>

                    </androidx.cardview.widget.CardView>
                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>


        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>